  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/index.js",
      "env": {
        "NODE_OPTIONS": "--openssl-legacy-provider"
      },
      "args": ["--experimental-fetch"],
      "preLaunchTask": "Compile"
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "restart": true,
      "port": 5858,
      "outFiles": [],
      "sourceMaps": true
    }
  ]
}
